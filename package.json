{
  "author": "Girma Nigusse <xgirma@gmail.com>",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "bugs": {
    "url": "https://github.com/e2e-boilerplate/selenium-webdriver-typescript-ts-node-ava/issues"
  },
  "dependencies": {
    "ava": "3.8.2",
    "chromedriver": "83.0.0",
    "selenium-webdriver": "4.0.0-alpha.7",
    "ts-node": "8.10.2",
    "typescript": "3.9.5",
    "webdriver-manager": "12.1.7"
  },
  "description": "Selenium-WebDriver end-to-end test automation boilerplate. Using TypeScript, ts-node and AVA.",
  "devDependencies": {
    "@types/node": "13.13.12",
    "@types/selenium-webdriver": "4.0.9",
    "husky": "4.2.5",
    "lint-staged": "10.2.10",
    "prettier": "2.0.4",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0"
  },
  "homepage": "https://github.com/e2e-boilerplate/selenium-webdriver-typescript-ts-node-ava#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "keywords": [
    "ava",
    "selenium",
    "selenium-webdriver",
    "selenium-webdriver automation",
    "selenium-webdriver boilerplate",
    "selenium-webdriver example",
    "ts-node",
    "typescript",
    "webdriver manager",
    "e2e",
    "e2e tests",
    "boilerplate",
    "integration test",
    "test automation",
    "javascript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "tslint --fix",
      "prettier --write"
    ]
  },
  "name": "selenium-webdriver-typescript-ts-node-ava",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-boilerplate/selenium-webdriver-typescript-ts-node-ava.git"
  },
  "scripts": {
    "lint": "tslint -c tslint.json './spec/*.ts'",
    "pretty": "prettier --no-config './spec/*.ts' --write",
    "test": "npx ava --timeout=1m ./spec/*.spec.ts"
  },
  "version": "1.0.0"
}