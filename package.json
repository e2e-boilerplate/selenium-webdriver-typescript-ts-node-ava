{
  "name": "selenium-webdriver-typescript-ts-node-ava",
  "version": "1.0.0",
  "description": "selenium-webdriver end-to-end test automation boilerplate, using TypeScript, ts-node and AVA.",
  "devDependencies": {
    "@types/node": "12.12.26",
    "@types/selenium-webdriver": "4.0.6",
    "ava": "3.1.0",
    "chromedriver": "79.0.2",
    "husky": "4.2.1",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "selenium-webdriver": "4.0.0-alpha.5",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "pretty": "prettier --no-config './spec/*.ts' --write",
    "lint": "tslint -c tslint.json './spec/*.ts'",
    "test": "npx ava --timeout=1m ./spec/*.spec.ts"
  },
  "keywords": [
    "selenium-webdriver",
    "typescript",
    "ts-node",
    "AVA",
    "boilerplate",
    "example",
    "e2e"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
